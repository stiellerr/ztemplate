{
    "name": "ztemplate",
    "version": "1.0.0",
    "description": "Basic template for compiling scss, react, es6 + image optimization, linting and prettifying",
    "main": "index.js",
    "scripts": {
        "checkFormat": "prettier --check \"**/*.{js,json,css,scss}\"",
        "format": "prettier --write \"**/*.{js,json,css,scss}\"",
        "lint": "eslint src",
        "start": "webpack --mode=development --watch",
        "build": "npm run lint && npm run format && webpack --mode=production"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-react": "^7.14.5",
        "autoprefixer": "^10.3.1",
        "babel-loader": "^8.2.2",
        "css-loader": "^6.2.0",
        "css-minimizer-webpack-plugin": "^3.0.2",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-webpack-plugin": "^3.0.1",
        "file-loader": "^6.2.0",
        "imagemin-mozjpeg": "file:/./temp/imagemin-mozjpeg-9.0.0.tgz",
        "image-minimizer-webpack-plugin": "^2.2.0",
        "mini-css-extract-plugin": "^2.2.0",
        "mozjpeg": "file:/./temp/mozjpeg-7.1.0.tgz",
        "postcss-loader": "^6.1.1",
        "prettier": "^2.3.2",
        "sass": "^1.37.5",
        "sass-loader": "^12.1.0",
        "webpack": "^5.49.0",
        "webpack-cli": "^4.7.2"
    },
    "browserslist": [
        "last 5 versions"
    ],
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.4",
        "@popperjs/core": "^2.9.3",
        "bootstrap": "^5.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
